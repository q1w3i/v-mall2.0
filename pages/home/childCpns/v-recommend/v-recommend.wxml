<!--pages/home/childCpns/v-recommend/v-recommend.wxml-->
<view class="recommend">
  <block wx:for="{{list}}" wx:key="index">
    <view class="recommend-item">
      <image class="recommend-item-image" 
      src="{{item.image}}" 
      bind:load="handleImageLoad"
      alt=""/>
      <text class="recommend-item-text">{{item.title}}</text>
    </view>
  </block>
</view>
